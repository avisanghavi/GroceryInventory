[webservers]
web1 ansible_host=production-web1.example.com
web2 ansible_host=production-web2.example.com

[apiservers]
api1 ansible_host=production-api1.example.com
api2 ansible_host=production-api2.example.com

[dbservers]
db1 ansible_host=production-db1.example.com

[all:vars]
ansible_user=deploy
ansible_ssh_private_key_file=~/.ssh/deploy_key
ansible_python_interpreter=/usr/bin/python3 